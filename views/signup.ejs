<div class="col-sm-5">
    <h2>Signup</h2>
    <form method="post" action="/signup">
        username: <input type="text" autocomplete="username" name="username"><br>
        email: <input type="text" autocomplete="email" name="email"><br>
        image link: <input type="text" autocomplete="" name="image"><br>
        password
        <input type="password" autocomplete="passphrase" name="passphrase"><br>
        confirm password:
        <input type="password" name="passphrase2"><br>
        age: <input type="number" name="age"><br>
        <input type="submit">
    </form>
</div>